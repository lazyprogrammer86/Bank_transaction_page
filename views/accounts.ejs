<%-include("partials/header")-%>
<section id="accounts">
    <div class="user">
        <span><a type="button" href="/pay">Pay</a> <a href="/history">History</a> </span>
        <span id="user"><h1>User :Some Foundation</h1><h1>Amount : <%=sfbal%></h1></span>
    </div>
    <table>
        <tbody>
        <tr>
            <th>Serial Number</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Account Number</th>
            <th>Email ID</th>
            <th>Balance</th>
        </tr>
        <%for(var i=0;i<(account.length);i++){%>
        <tr>
            <td><%=i+1%></td>
            <td><%=account[i].fname%></td>
            <td><%=account[i].lname%></td>
            <td><%=account[i].accNumber%></td>
            <td><%=account[i].email%></td>
            <td><%=account[i].balance%>.00$</td>
        </tr>
        <%}%>
    </tbody>
    </table>
</section>

<%-include("partials/footer")-%>
